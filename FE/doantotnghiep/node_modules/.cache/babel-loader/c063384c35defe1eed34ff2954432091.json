JA;;AAKA;AACA;AACA;AACA,IAAA,YARA,0BAQA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,EAAA,CAAA,WAAA,GAAA,KAAA;AACA,MAAA,EAAA,CAAA,KAAA,CAAA,iBAAA,EAAA,EAAA,CAAA,KAAA,EAAA,EAAA,CAAA,WAAA,CAAA,aAAA;AACA,MAAA,EAAA,CAAA,KAAA,CAAA,YAAA;AACA,KAbA;;AAcA;AACA;AACA;AACA,IAAA,UAjBA,wBAiBA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,EAAA,CAAA,WAAA,GAAA,KAAA;AACA,MAAA,EAAA,CAAA,KAAA,CAAA,YAAA;AACA,KArBA;AAsBA,IAAA,OAtBA,qBAsBA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,UAAA,QAAA,GAAA,EAAA,CAAA,KAAA,CAAA,OAAA,CAAA,EAAA,CAAA,IAAA,KAAA,CAAA;;AACA,UAAA,QAAA,EAAA;AACA,YAAA,CAAA,GAAA,EAAA,CAAA,cAAA;AACA,aAAA,OAAA,CAAA,KAAA,CAAA;AACA,UAAA,KAAA,EAAA,EAAA,CAAA,EAAA,CAAA,cAAA,CADA;AAEA,UAAA,OAAA,EAAA,CAAA,CACA,GADA,EAEA;AAAA,YAAA,KAAA,EAAA;AAAA,WAFA,EAGA,EAAA,CAAA,EAAA,CAAA,mBAAA,CAHA;AAFA,SAAA;AAQA,OAVA,MAUA;AACA,YAAA,EAAA,CAAA,IAAA,KAAA,EAAA,EAAA;AAEA,UAAA,EAAA,CAAA,KAAA,CAAA,OAAA,CAAA,EAAA,CAAA,IAAA;AACA,UAAA,EAAA,CAAA,IAAA,GAAA,EAAA;AACA;AACA;AACA,KA1CA;AA2CA,IAAA,UA3CA,sBA2CA,KA3CA,EA2CA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,EAAA,CAAA,KAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA;AA9CA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <el-dialog\r\n    ref=\"dialog\"\r\n    title=\"Thêm IMEI\"\r\n    :visible.sync=\"modelDialog\"\r\n  >\r\n    <template #default>\r\n        <div class=\"form\">\r\n            <div class=\"label-text\">Nhập IMEI</div>\r\n            <div class=\"text-field\">\r\n                <el-input v-model=\"imei\" @change=\"addIMEI\"></el-input>\r\n            </div>\r\n        </div>\r\n        <div class=\"imei-list\">\r\n            <div class=\"imei-item tw-flex tw-items-center\" v-for=\"(item,index) in imeis\" :key=\"index\">\r\n                <div class=\"imei-text\">{{item}}</div>\r\n                <el-button class=\"button-delete\" @click=\"deleteEMEI(index)\">X</el-button>\r\n            </div>\r\n        </div>\r\n    </template>\r\n    <template #footer>\r\n      <div class=\"button tw-flex\">\r\n          <el-button @click=\"completeFill\" class=\"complete-btn\">{{\r\n        $t(\"Button.Complete\")\r\n      }}</el-button>\r\n          <el-button @click=\"cancelFill\" class=\"cancel-btn\">{{\r\n        $t(\"Button.Cancel\")\r\n      }}</el-button>\r\n      </div>\r\n    </template>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    props:{\r\n        orderDetail:{\r\n\r\n            type:Object,\r\n            default:()=>{}\r\n        }\r\n    },\r\ndata() {\r\n    return {\r\n      modelDialog: false,\r\n      modelForm: [],\r\n      active:0,\r\n      rules:{},\r\n      imei:'',\r\n      imeis:[]\r\n    };\r\n  },\r\n  mounted(){\r\n      const me=this\r\n      me.imeis=JSON.parse(JSON.stringify(me.orderDetail.IMEI)).split(',')\r\n  },\r\n  methods: {\r\n    setModel(value) {\r\n      const me = this;\r\n      me.modelDialog = value;\r\n    },\r\n    /**\r\n     * Hoàn thành thêm thông tin chi tiết sản phẩm\r\n     */\r\n    completeFill(){\r\n        const me=this\r\n        me.modelDialog = false;\r\n        me.$emit('updateIMEIValue',me.imeis,me.orderDetail.OrderDetailID)\r\n        me.$emit('closePopup')\r\n    },\r\n    /**\r\n     * Hủy thêm thôn gtin chi tiết sản phẩm\r\n     */\r\n    cancelFill(){\r\n        const me=this\r\n        me.modelDialog = false;\r\n        me.$emit('closePopup')\r\n    },\r\n    addIMEI(){\r\n        const me=this\r\n        const isExists=me.imeis.indexOf(me.imei)>=0\r\n        if(isExists){\r\n            const h= me.$createElement\r\n            this.$notify.error({\r\n                  title: me.$t(\"Notify.Error\"),\r\n                  message: h(\r\n                    \"i\",\r\n                    { style: \"color: teal\" },\r\n                    me.$t(\"Notify.ExistsIMEI\")\r\n                  ),\r\n                });\r\n        }else{\r\n            if(me.imei!==''){\r\n\r\n              me.imeis.unshift(me.imei)\r\n              me.imei=''\r\n            }\r\n        }\r\n    },\r\n    deleteEMEI(index){\r\n        const me=this\r\n        me.imeis.splice(index,1)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.button-delete{\r\n  @apply tw-w-10 tw-mt-0 tw-ml-3 !important;\r\n}\r\n</style>"],"sourceRoot":"src/components/admin/views/products"}]}