.LessThanTwo\"\r\n          hide-details\r\n          @change=\"onCheckedItem(filterType.Price)\"\r\n        ></v-checkbox>\r\n        <v-checkbox\r\n          class=\"my-checkbox\"\r\n          label=\"Từ 2-4 triệu\"\r\n          color=\"red\"\r\n          v-model=\"checkedValuePrice\"\r\n          :value=\"priceValue.TwoToFour\"\r\n          hide-details\r\n          @change=\"onCheckedItem(filterType.Price)\"\r\n        ></v-checkbox>\r\n        <v-checkbox\r\n          class=\"my-checkbox\"\r\n          label=\"Từ 4-7 triệu\"\r\n          color=\"red\"\r\n          v-model=\"checkedValuePrice\"\r\n          :value=\"priceValue.FourToSeven\"\r\n          hide-details\r\n          @change=\"onCheckedItem(filterType.Price)\"\r\n        ></v-checkbox>\r\n        <v-checkbox\r\n          class=\"my-checkbox\"\r\n          label=\"Từ 7-15 triệu\"\r\n          color=\"red\"\r\n          v-model=\"checkedValuePrice\"\r\n          :value=\"priceValue.SevenToFifteen\"\r\n          hide-details\r\n          @change=\"onCheckedItem(filterType.Price)\"\r\n        ></v-checkbox>\r\n        <v-checkbox\r\n          class=\"my-checkbox\"\r\n          label=\"Trên 15 triệu\"\r\n          color=\"red\"\r\n          v-model=\"checkedValuePrice\"\r\n          :value=\"priceValue.OverFifteen\"\r\n          hide-details\r\n          @change=\"onCheckedItem(filterType.Price)\"\r\n        ></v-checkbox>\r\n      </div>\r\n    </div>\r\n    <div class=\"monitor tw-mb-3\">\r\n      <div class=\"filter-type tw-text-base\">Màn hình</div>\r\n      <div class=\"property\">\r\n        <v-checkbox\r\n          class=\"my-checkbox\"\r\n          label=\"Tất cả\"\r\n          color=\"red\"\r\n          :input-value=\"isCheckAllMonitor\"\r\n          hide-details\r\n          @change=\"checkAll(filterType.Monitor)\"\r\n        ></v-checkbox>\r\n        <v-checkbox\r\n          class=\"my-checkbox\"\r\n          label=\"Dưới 6.0 inch\"\r\n          color=\"red\"\r\n          v-model=\"checkedValueMonitor\"\r\n          :value=\"monitorValue.LessThanSix\"\r\n          hide-details\r\n          @change=\"onCheckedItem(filterType.Monitor)\"\r\n        ></v-checkbox>\r\n        <v-checkbox\r\n          class=\"my-checkbox\"\r\n          label=\"Trên 6.0 inch\"\r\n          color=\"red\"\r\n          v-model=\"checkedValueMonitor\"\r\n          :value=\"monitorValue.OverSix\"\r\n          hide-details\r\n          @change=\"onCheckedItem(filterType.Monitor)\"\r\n        ></v-checkbox>\r\n      </div>\r\n    </div>\r\n    <div class=\"battery tw-mb-3\">\r\n      <div class=\"filter-type tw-text-base\">Dung lượng pin</div>\r\n      <div class=\"property\">\r\n        <v-checkbox\r\n          class=\"my-checkbox\"\r\n          label=\"Tất cả\"\r\n          color=\"red\"\r\n          :input-value=\"isCheckAllBattery\"\r\n          hide-details\r\n          @change=\"checkAll(filterType.Battery)\"\r\n        ></v-checkbox>\r\n        <v-checkbox\r\n          class=\"my-checkbox\"\r\n          label=\"Dưới 3000 mah\"\r\n          color=\"red\"\r\n          v-model=\"checkedValueBattery\"\r\n          :value=\"batteryValue.LessThanThree\"\r\n          hide-details\r\n          @change=\"onCheckedItem(filterType.Battery)\"\r\n        ></v-checkbox>\r\n        <v-checkbox\r\n          class=\"my-checkbox\"\r\n          label=\"Từ 3000-5000 mah\"\r\n          color=\"red\"\r\n          v-model=\"checkedValueBattery\"\r\n          :value=\"batteryValue.ThreeToFive\"\r\n          hide-details\r\n          @change=\"onCheckedItem(filterType.Battery)\"\r\n        ></v-checkbox>\r\n        <v-checkbox\r\n          class=\"my-checkbox\"\r\n          label=\"Trên 5000 mah\"\r\n          color=\"red\"\r\n          v-model=\"checkedValueBattery\"\r\n          :value=\"batteryValue.OverFive\"\r\n          hide-details\r\n          @change=\"onCheckedItem(filterType.Battery)\"\r\n        ></v-checkbox>\r\n      </div>\r\n    </div>\r\n    <img\r\n      src=\"https://cdn-thumbs.imagevenue.com/c9/b6/08/ME14JIL0_t.png\"\r\n      alt=\"Success.png\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions, mapState, mapMutations } from \"vuex\";\r\nexport default {\r\n  name: \"FilterProduct\",\r\n  data() {\r\n    return {\r\n      ex4: true,\r\n      isCheckAllManufacturer: true,\r\n      filter: [\"\", \"\", \"\", \"\"],\r\n      prices: { All: true },\r\n      batteries: { All: true },\r\n      monitors: { All: true },\r\n      priceNumber: 0,\r\n      checkedValueManufacturer: [],\r\n      checkedValuePrice: [],\r\n      checkedValueMonitor: [],\r\n      checkedValueBattery: [],\r\n      priceValue: this.$enumeration.FilterPrice,\r\n      monitorValue: this.$enumeration.FilterMonitor,\r\n      batteryValue: this.$enumeration.FilterBattery,\r\n      filterType: this.$enumeration.Filter,\r\n    };\r\n  },\r\n  created() {\r\n    const me = this;\r\n    me.getManufacturers();\r\n  },\r\n  computed: {\r\n    ...mapState(\"category\", [\"categories\"]),\r\n    ...mapState(\"manufacturer\", [\"manufacturers\"]),\r\n    //hiển thị bộ lọc theo hãng nếu là là phụ kiện\r\n    showManufacturer() {\r\n      const me = this;\r\n      return me.$route.query.accessory ||\r\n        me.$route.params.category === \"phu-kien\"\r\n        ? true\r\n        : false;\r\n    },\r\n    /**\r\n     * check All hãng săn xuất\r\n     */\r\n    isCheckAllManufacturerclone() {\r\n      return this.checkedValueManufacturer.length == 0;\r\n    },\r\n    /**\r\n     * check All giá\r\n     */\r\n    isCheckAllPrice() {\r\n      return this.checkedValuePrice.length == 0;\r\n    },\r\n    /**\r\n     * check All màn hình\r\n     */\r\n    isCheckAllMonitor() {\r\n      return this.checkedValueMonitor.length == 0;\r\n    },\r\n    /**\r\n     * check All pin\r\n     */\r\n    isCheckAllBattery() {\r\n      return this.checkedValueBattery.length == 0;\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapActions(\"manufacturer\", [\"getManufacturers\"]),\r\n    ...m