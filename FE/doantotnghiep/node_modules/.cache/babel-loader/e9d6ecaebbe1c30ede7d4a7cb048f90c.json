BA;AAsBA,EAAA,OAAA,kCACA,UAAA,CAAA,OAAA,EAAA,CAAA,aAAA,EAAA,kBAAA,EAAA,aAAA,CAAA,CADA;AAEA,IAAA,cAFA,0BAEA,SAFA,EAEA,QAFA,EAEA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,UAAA,MAAA,GAAA;AACA,QAAA,OAAA,EAAA,EAAA,CAAA,MAAA,CAAA,KAAA,CAAA,EADA;AAEA,QAAA,SAAA,EAAA,SAFA;AAGA,QAAA,QAAA,EAAA;AAHA,OAAA;AAKA,MAAA,EAAA,CAAA,WAAA,CAAA,MAAA;AACA,KAVA;AAWA,IAAA,gBAXA,4BAWA,GAXA,EAWA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,EAAA,CAAA,QAAA,GAAA,GAAA;AACA,MAAA,EAAA,CAAA,cAAA,CAAA,EAAA,CAAA,SAAA,EAAA,EAAA,CAAA,QAAA;AACA,KAfA;AAgBA,IAAA,mBAhBA,+BAgBA,GAhBA,EAgBA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,EAAA,CAAA,SAAA,GAAA,GAAA;AACA,MAAA,EAAA,CAAA,cAAA,CAAA,EAAA,CAAA,SAAA,EAAA,EAAA,CAAA,QAAA;AACA,KApBA;AAqBA,IAAA,cArBA,0BAqBA,WArBA,EAqBA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,UAAA,eAAA,GAAA,EAAA,CAAA,YAAA,CAAA,WAAA;;AACA,cAAA,WAAA;AACA,aAAA,eAAA,CAAA,WAAA;AACA,iBAAA,cAAA;;AACA,aAAA,eAAA,CAAA,SAAA;AACA,iBAAA,cAAA;;AACA,aAAA,eAAA,CAAA,UAAA;AACA,iBAAA,gBAAA;;AACA,aAAA,eAAA,CAAA,SAAA;AACA,iBAAA,cAAA;;AACA,aAAA,eAAA,CAAA,SAAA;AACA,iBAAA,QAAA;;AACA;AACA,iBAAA,KAAA;AAZA;AAcA,KAtCA;AAuCA,IAAA,SAvCA,qBAuCA,KAvCA,EAuCA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,EAAA,CAAA,MAAA,GAAA,KAAA;AACA,MAAA,KAAA,CAAA,KAAA,CAAA;AACA,KA3CA;AA4CA,IAAA,QA5CA,sBA4CA;AACA,UAAA,EAAA,GAAA,IAAA;AACA;AACA,MAAA,EAAA,CAAA,MAAA;AACA,UAAA,QAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,EAAA,CAAA,aAAA,CAAA,KAAA,CAAA,CAAA;AACA,MAAA,QAAA,CAAA,WAAA,GAAA,EAAA,CAAA,MAAA;AACA,UAAA,MAAA,GAAA;AACA,QAAA,EAAA,EAAA,QAAA,CAAA,OADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA;AAIA,MAAA,EAAA,CAAA,WAAA,CAAA,MAAA;AACA;AAvDA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"order-detail-container tw-relative\">\r\n    <div class=\"order-info tw-grid tw-grid-cols-2 tw-grid-rows-1 tw-p-6\">\r\n      <div class=\"order-content\">\r\n        <div class=\"order\"></div>\r\n        <div class=\"order-code\">\r\n          <span class=\"label\">Mã đơn hàng</span>\r\n          <span class=\"tw-ml-6\">{{ orderCustomer.Order.OrderID }}</span>\r\n        </div>\r\n        <div class=\"product-quantity\">\r\n          <span class=\"label\">Số lượng mặt hàng</span>\r\n          <span class=\"tw-ml-6\">{{ orderCustomer.Quantity }}</span>\r\n        </div>\r\n        <div class=\"price-total\">\r\n          <span class=\"label\">Tổng giá</span>\r\n          <span class=\"tw-ml-6\">{{ orderCustomer.Order.TotalPrice }}</span>\r\n        </div>\r\n\r\n        <div class=\"order-status\">\r\n          <span class=\"label\">Trạng thái đơn hàng</span>\r\n          <span class=\"tw-ml-6\">{{\r\n            getOrderStatus(orderCustomer.Order.OrderStatus)\r\n          }}</span>\r\n          <span>\r\n            <el-steps\r\n              :active=\"active\"\r\n              process-status=\"finish\"\r\n              finish-status=\"success\"\r\n              align-center\r\n            >\r\n              <el-step\r\n                v-for=\"index in 4\"\r\n                :key=\"index\"\r\n                @click=\"setActive(index)\"\r\n                :title=\"getOrderStatus(index)\"\r\n              ></el-step>\r\n            </el-steps>\r\n          </span>\r\n          <el-button\r\n            style=\"margin-top: 12px\"\r\n            :disabled=\"active === 3\"\r\n            @click=\"nextStep\"\r\n            >Next step</el-button\r\n          >\r\n        </div>\r\n        <div class=\"payment-status\">\r\n          <span class=\"label\">Trạng thái thanh toán</span>\r\n          <span class=\"tw-ml-6\">{{\r\n            orderCustomer.Order.PaymentStatus\r\n              ? \"Đã thanh toán\"\r\n              : \"Chưa thanh toán\"\r\n          }}</span>\r\n        </div>\r\n        <div class=\"order-date\">\r\n          <span class=\"label\">Ngày đặt hàng</span>\r\n          <span class=\"tw-ml-6\">{{ orderCustomer.Order.CreatedDate }}</span>\r\n        </div>\r\n      </div>\r\n      <div class=\"customer-content\">\r\n        <div class=\"customer-name\">\r\n          <span class=\"label\">Tên khách hàng</span>\r\n          <span class=\"tw-ml-6\">{{ orderCustomer.Customer.CustomerName }}</span>\r\n        </div>\r\n        <div class=\"phone-number\">\r\n          <span class=\"label\">SĐT khách hàng</span>\r\n          <span class=\"tw-ml-6\">{{ orderCustomer.Customer.PhoneNumber }}</span>\r\n        </div>\r\n        <div class=\"email\">\r\n          <span class=\"label\">Email</span>\r\n          <span class=\"tw-ml-6\">{{ orderCustomer.Customer.Email }}</span>\r\n        </div>\r\n        <div class=\"address\">\r\n          <span class=\"label\">Địa chỉ nhận hàng</span>\r\n          <span class=\"tw-ml-6\">{{ orderCustomer.Order.DeliveryAddress }}</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div\r\n      class=\"order-detail-list tw-absolute tw-bottom-0 tw-w-full tw-p-6\"\r\n      style=\"height: calc(100% - 115px - 49px - 17px)\"\r\n    >\r\n      <el-table\r\n        class=\"table\"\r\n        border\r\n        :data=\"orderDetails\"\r\n        height=\"calc(100% - 49px - 16px)\"\r\n        style=\"width: 100%\"\r\n      >\r\n        <el-table-column type=\"index\" width=\"50\" label=\"STT\"> </el-table-column>\r\n        <el-table-column\r\n          prop=\"OrderDetail.OrderDetailID\"\r\n          label=\"Mã mặt hàng\"\r\n          width=\"150\"\r\n        >\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"Product.ProductName\"\r\n          label=\"Tên mặt hàng\"\r\n          min-width=\"200\"\r\n        >\r\n        </el-table-column>\r\n        <el-table-column prop=\"Product.Image\" label=\"Ảnh\" width=\"200\">\r\n          <template slot-scope=\"scope\">\r\n            <div class=\"image tw-w-20 tw-h-20\">\r\n              <el-image :src=\"scope.row.Product.Image\" fit=\"contain\">\r\n                <div slot=\"placeholder\" class=\"image-slot\">\r\n                  {{ $t(\"Common.Loading\") }}<span class=\"dot\">...</span>\r\n                </div>\r\n              </el-image>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"OrderDetail.Quantity\"\r\n          label=\"Số lượng\"\r\n          width=\"150\"\r\n        >\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"OrderDetail.UnitPrice\"\r\n          label=\"Đơn giá\"\r\n          width=\"200\"\r\n        >\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"ProductAttribute.Memory\"\r\n          label=\"Dung lượng\"\r\n          width=\"150\"\r\n        >\r\n        </el-table-column>\r\n        <el-table-column label=\"Thành tiên\" width=\"200\">\r\n          <template slot-scope=\"scope\">\r\n            {{\r\n              scope.row.OrderDetail.Quantity * scope.row.OrderDetail.UnitPrice\r\n            }}\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <el-pagination\r\n        class=\"tw-flex tw-justify-end tw-h-12 tw-items-center\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page.sync=\"pageIndex\"\r\n        :page-size=\"pageSize\"\r\n        :page-sizes=\"pageSizes\"\r\n        layout=\"total,sizes, prev, pager, next\"\r\n        :total=\"orderDetailTotal\"\r\n      >\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapActions } from \"vuex\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      pageSizes: [5, 10, 15],\r\n      pageSize: 5,\r\n      pageIndex: 1,\r\n      active: 0,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState(\"order\", [\r\n      \"orderDetails\",\r\n      \"orderDetailTotal\",\r\n      \"pageOrderDetailTotal\",\r\n      \"orderCustomer\",\r\n    ]),\r\n  },\r\n  created() {\r\n    const me = this;\r\n    me.getOrderDetail(me.pageIndex, me.pageSize);\r\n    me.getCustomerOrder({ orderID: me.$route.query.id });\r\n  },\r\n  methods: {\r\n    ...mapActions(\"order\", [\"detailOrder\", \"getCustomerOrder\",\"updateOrder\"]),\r\n    getOrderDetail(pageIndex, pageSize) {\r\n      const me = this;\r\n      const params = {\r\n        orderID: me.$route.query.id,\r\n        pageIndex: pageIndex,\r\n        pageSize: pageSize,\r\n      };\r\n      me.detailOrder(params);\r\n    },\r\n    handleSizeChange(val) {\r\n      const me = this;\r\n      me.pageSize = val;\r\n      me.getOrderDetail(me.pageIndex, me.pageSize);\r\n    },\r\n    handleCurrentChange(val) {\r\n      const me = this;\r\n      me.pageIndex = val;\r\n      me.getOrderDetail(me.pageIndex, me.pageSize);\r\n    },\r\n    getOrderStatus(orderStatus) {\r\n      const me = this;\r\n      const enumOrderStatus = me.$enumeration.OrderStatus;\r\n      switch (orderStatus) {\r\n        case enumOrderStatus.WaitConfirm:\r\n          return \"Chờ xác nhận\";\r\n        case enumOrderStatus.WaitGoods:\r\n          return \"Chờ lấy hàng\";\r\n        case enumOrderStatus.Delivering:\r\n          return \"Đang giao hàng\";\r\n        case enumOrderStatus.Delivered:\r\n          return \"Đã giao hàng\";\r\n        case enumOrderStatus.Cancelled:\r\n          return \"Đã hủy\";\r\n        default:\r\n          return \"abc\";\r\n      }\r\n    },\r\n    setActive(index) {\r\n      const me = this;\r\n      me.active = index;\r\n      alert(index);\r\n    },\r\n    nextStep() {\r\n      const me = this;\r\n      debugger\r\n      me.active++;\r\n      const newOrder=JSON.parse(JSON.stringify(me.orderCustomer.Order))\r\n      newOrder.OrderStatus=me.active\r\n      const params={\r\n        id:newOrder.OrderID,\r\n        params:newOrder\r\n      }\r\n      me.updateOrder(params)\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.label {\r\n  font-size: 14px;\r\n  font-weight: 600;\r\n}\r\n</style>"],"sourceRoot":"src/components/admin/views/orders"}]}