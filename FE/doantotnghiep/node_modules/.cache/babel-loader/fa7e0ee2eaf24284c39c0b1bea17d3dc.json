lasses: {\r\n      default: '',\r\n      type: String\r\n    },\r\n    styles: {\r\n      type: Object,\r\n      default: () => {}\r\n    },\r\n    plugins: {\r\n      type: Array,\r\n      default: () => []\r\n    },\r\n    chartData: {\r\n      type: Array,\r\n      default: () => []\r\n    },\r\n    labels: {\r\n      type: Array,\r\n      default: () => []\r\n    },\r\n    chartTitle:{\r\n      type:String,\r\n      default:''\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      \r\n      chartOptions: {\r\n        responsive: true,\r\n        maintainAspectRatio: false\r\n      }\r\n    }\r\n  },\r\n  computed:{\r\n    data() {\r\n      return{\r\n\r\n        labels: this.labels,\r\n        datasets: [\r\n          {\r\n            backgroundColor: ['#41B883', '#E46651', '#00D8FF', '#DD1B16'],\r\n            data: this.chartData\r\n          }\r\n        ]\r\n      }\r\n      },\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":"src/components/controls/chart"}]}