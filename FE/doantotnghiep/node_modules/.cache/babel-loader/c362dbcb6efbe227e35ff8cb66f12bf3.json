data){\r\n            state.productBestSellings=data\r\n        },\r\n        setCancelledOrders(state,data){\r\n            state.cancelledOrders=data\r\n        },\r\n        \r\n        \r\n    },\r\n    actions:{\r\n        \r\n        getTurnoverTotal({commit}){\r\n            OverviewApi.getTurnoverTotal().then((res)=>{\r\n                const data=res?.data?.Data\r\n                commit('setTurnoverTotal',data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        getOrderTotal({commit}){\r\n            OverviewApi.getOrderTotal().then((res)=>{\r\n                const data=res?.data?.Data\r\n                commit('setOrderTotal',data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        getReceiptTotal({commit}){\r\n            OverviewApi.getReceiptTotal().then((res)=>{\r\n                const data=res?.data?.Data\r\n                commit('setReceiptTotal',data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        getNewProductTotal({commit}){\r\n            OverviewApi.getNewProductTotal().then((res)=>{\r\n                const data=res?.data?.Data\r\n                commit('setNewProductTotal',data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        async getMonthlyTurnover({commit}){\r\n            \r\n            await OverviewApi.getMonthlyTurnover().then((res)=>{\r\n                const data=res?.data?.Data\r\n                const dataNew=[]\r\n                for (const property in data) {\r\n                    dataNew.push(data[property])\r\n                }\r\n                commit('setMonthlyTurnover',dataNew)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        getQuantitySoldByManufacturer({commit}){\r\n            \r\n             OverviewApi.getQuantitySoldByManufacturer().then((res)=>{\r\n                const data=res?.data?.Data\r\n                \r\n                commit('setQuantitySoldByManufacturer',data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        getQuantityOrderCancelled({commit}){\r\n            \r\n             OverviewApi.getQuantityOrderCancelled().then((res)=>{\r\n                const data=res?.data?.Data\r\n                \r\n                commit('setQuantityOrderCancelled',data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        getProductBestSellingsInMonth({commit},param){\r\n            \r\n             OverviewApi.getProductBestSellingsInMonth(param).then((res)=>{\r\n                const data=res?.data?.Data\r\n                \r\n                commit('setProductBestSellings',data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        getCancelledOrders({commit}){\r\n            \r\n             OverviewApi.getCancelledOrders().then((res)=>{\r\n                const data=res?.data?.Data\r\n                \r\n                commit('setCancelledOrders',data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        \r\n        \r\n    }\r\n}"]}]}