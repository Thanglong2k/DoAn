;AACA,QAAA,YAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,CAAA;;AACA,QAAA,YAAA,CAAA,OAAA,IAAA,YAAA,CAAA,QAAA,EAAA;AACA,MAAA,EAAA,CAAA,SAAA,CAAA,QAAA,GAAA,IAAA,CAAA,YAAA,CAAA,OAAA,CAAA;AACA,MAAA,EAAA,CAAA,SAAA,CAAA,QAAA,GAAA,IAAA,CAAA,YAAA,CAAA,QAAA,CAAA;AACA,MAAA,EAAA,CAAA,gBAAA,GAAA,IAAA;AACA;AACA,GA5BA;AA6BA,EAAA,OAAA,kCACA,UAAA,CAAA,SAAA,EAAA,CAAA,OAAA,CAAA,CADA;AAEA;AACA;AACA;AACA,IAAA,WALA,yBAKA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,EADA,GACA,KADA;AAEA;AACA,gBAAA,WAHA,GAGA,EAHA;AAAA;AAAA,uBAIA,EAAA,CAAA,KAAA,CAAA,EAAA,CAAA,SAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA,CACA,CADA,CAJA;;AAAA;AAAA,8BAMA,EAAA,CAAA,aANA;AAAA,gDAOA,EAAA,CAAA,SAAA,CAAA,KAAA,CAAA,SAPA,uBAUA,EAAA,CAAA,SAAA,CAAA,KAAA,CAAA,MAVA;AAAA;;AAAA;AAQA,gBAAA,EAAA,CAAA,QAAA,GAAA,oCAAA;AARA;;AAAA;AAWA,gBAAA,EAAA,CAAA,QAAA,GAAA,sBAAA;AAXA;;AAAA;AAcA;AACA,gBAAA,WAAA,GAAA;AACA,kBAAA,QAAA,EAAA,EAAA,CAAA,OAAA,CAAA,QADA;AAEA,kBAAA,QAAA,EAAA,EAAA,CAAA,OAAA,CAAA,QAFA;AAGA,kBAAA,UAAA,EAAA,EAAA,CAAA,OAAA,CAAA;AAHA,iBAAA;AAfA;AAAA,uBAoBA,cAAA,CAAA,OAAA,CAAA,SAAA,EAAA,IAAA,CAAA,SAAA,CAAA,WAAA,CAAA,CApBA;;AAAA;AAqBA,oBAAA,EAAA,CAAA,gBAAA,EAAA;AACA,kBAAA,OADA,GACA;AACA,oBAAA,OAAA,EAAA,IAAA,CAAA,EAAA,CAAA,SAAA,CAAA,QAAA,CADA;AAEA,oBAAA,QAAA,EAAA,IAAA,CAAA,EAAA,CAAA,SAAA,CAAA,QAAA,CAFA;AAGA,oBAAA,QAAA,EAAA;AAHA,mBADA;AAMA,kBAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,OAAA,CAAA;AACA,iBAPA,MAOA;AACA,kBAAA,YAAA,CAAA,UAAA,CAAA,OAAA;AACA;;AA9BA,sBAgCA,EAAA,CAAA,MAAA,CAAA,QAAA,KAAA,QAhCA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAiCA,EAAA,CAAA,OAAA,CAAA,IAAA,CAAA,iBAAA,CAjCA;;AAAA;AAAA;AAAA;;AAAA;AAoCA,gBAAA,EAAA,CAAA,OAAA,CAAA,EAAA;;AApCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAyCA;AA9CA;AA7BA,CAAA","sourcesContent":["<template>\r\n  <div class=\"login-container tw-bg-red-300 tw-flex tw-items-center tw-justify-center\">\r\n    <div\r\n      class=\"\r\n        login-form tw-bg-white tw-rounded-lg tw-flex tw-flex-col tw-items-center tw-p-8\r\n      \"\r\n    >\r\n      <div class=\"login-title tw-text-red-500 tw-font-bold tw-text-3xl\">\r\n        ĐĂNG NHẬP\r\n      </div>\r\n      <span class=\"tw-mt-3 tw-text-red-600\">{{ msgError }}</span>\r\n      <div class=\"login-body tw-w-full\">\r\n        <el-form\r\n          :model=\"modelForm\"\r\n          :rules=\"rules\"\r\n          ref=\"ruleForm\"\r\n          label-width=\"120px\"\r\n          label-position=\"top\"\r\n          class=\"demo-ruleForm\"\r\n        >\r\n          <el-form-item class=\"item-form\" :label=\"$t('Input.UserName')\" prop=\"UserName\">\r\n            <!-- <label>{{$t('Input.FullName')}}<span class=\"tw-text-red-400\">*</span></label> -->\r\n            <el-input\r\n              v-model=\"modelForm.UserName\"\r\n              :placeholder=\"$t('Input.UserNameInput')\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item class=\"item-form\" :label=\"$t('Input.Password')\" prop=\"Password\">\r\n            <!-- <label>{{$t('Input.PhoneNumber')}}<span class=\"tw-text-red-400\">*</span></label> -->\r\n            <el-input\r\n              v-model=\"modelForm.Password\"\r\n              :placeholder=\"$t('Input.PasswordInput')\"\r\n              type=\"password\"\r\n              show-password\r\n            />\r\n          </el-form-item>\r\n          <el-form-item class=\"item-form\">\r\n            <!-- <label>{{$t('Input.PhoneNumber')}}<span class=\"tw-text-red-400\">*</span></label> -->\r\n            <el-checkbox v-model=\"rememberPassword\">Nhớ mật khẩu</el-checkbox>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div class=\"button tw-w-full tw-flex tw-justify-center tw-mt-6\">\r\n          <el-button class=\"login-btn\" @click=\"handleLogin\"\r\n            >Đăng nhập</el-button\r\n          >\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapActions } from \"vuex\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      modelForm: {},\r\n      rules: {\r\n        UserName: [\r\n          { required: true, message: \"Hãy nhập thông tin!\", trigger: \"blur\" },\r\n          { min: 3, message: \"Tên tối thiểu 3 kí tự\", trigger: \"blur\" },\r\n        ],\r\n        Password: [\r\n          { required: true, message: \"Hãy nhập thông tin!\", trigger: \"blur\" },\r\n        ],\r\n      },\r\n      msgError:'',\r\n      rememberPassword:false\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState(\"account\", [\"statusAccount\",\"account\"]),\r\n  },\r\n  created(){\r\n    const me=this\r\n    const accountLocal= JSON.parse(localStorage.getItem(\"login\"))\r\n    if(accountLocal.usename && accountLocal.password){\r\n      me.modelForm.UserName=atob(accountLocal.usename)\r\n      me.modelForm.Password=atob(accountLocal.password)\r\n      me.rememberPassword=true\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapActions(\"account\", [\"login\"]),\r\n    /**\r\n     * Xử lý đăng nhập\r\n     */\r\n    async handleLogin() {\r\n      const me = this;\r\n      debugger\r\n      let sessAccount={}\r\n      await me.login(me.modelForm).then((res)=>{\r\n      });\r\n      switch (me.statusAccount) {\r\n        case me.$constant.Login.NotExists:\r\n          me.msgError = \"Tài khoản hoặc mật khẩu không đúng\";\r\n          break;\r\n        case me.$constant.Login.Locked:\r\n          me.msgError = \"Tài khoản đã bị khóa\";\r\n          break;\r\n        default:\r\n          debugger\r\n          sessAccount={\r\n            UserName:me.account.UserName,\r\n            FullName:me.account.FullName,\r\n            Permission:me.account.Permission\r\n          }\r\n          await sessionStorage.setItem('Account',JSON.stringify(sessAccount))\r\n          if(me.rememberPassword){\r\n            const account={\r\n              usename:btoa(me.modelForm.UserName),\r\n              password:btoa(me.modelForm.Password),\r\n              remember:true\r\n            }\r\n            localStorage.setItem('login',JSON.stringify(account))\r\n          }else{\r\n            localStorage.removeItem('login')\r\n          }\r\n          \r\n          if(me.$route.fullPath==='/login'){\r\n            await me.$router.push('/admin/overview');\r\n          }else{\r\n            \r\n            me.$router.go();\r\n          }\r\n          break;\r\n      }\r\n      \r\n    }\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.login-btn {\r\n  @apply tw-bg-red-500 tw-text-white !important;\r\n}\r\n</style>\r\n<style lang=\"scss\" scoped>\r\n.login-container {\r\n  height:100vh;\r\n  .login-form {\r\n        box-shadow: 0px 0px 10px -1px white;\r\n    width: 500px;\r\n    .login-body{\r\n      .item-form{\r\n        margin-bottom: 0px;\r\n        .el-form-item_label{\r\n          padding:0;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/components/admin/components/login"}]}