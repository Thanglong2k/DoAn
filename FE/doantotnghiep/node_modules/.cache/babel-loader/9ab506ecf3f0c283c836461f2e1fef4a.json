,CAAA,UAAA;;AACA,UAAA,EAAA,CAAA,UAAA,GAAA,EAAA,CAAA,KAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,QAAA,EAAA,CAAA,UAAA,GAAA,CAAA;AACA;AACA,KAdA;AAeA,IAAA,WAfA,uBAeA,KAfA,EAeA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,EAAA,CAAA,UAAA,GAAA,KAAA;AACA,KAlBA;AAmBA,IAAA,SAnBA,qBAmBA,KAnBA,EAmBA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAFA,CAGA;AACA;;AACA,UACA,EAAA,CAAA,UAAA,KAAA,CAAA,IAAA,KAAA,GAAA,CAAA,IACA,EAAA,CAAA,UAAA,KAAA,EAAA,CAAA,KAAA,CAAA,MAAA,GAAA,CAAA,IACA,KAAA,GAAA,EAAA,CAAA,KAAA,CAAA,MAAA,GAAA,CAAA,GAAA,CAFA,IAGA,EAAA,CAAA,UAAA,KAAA,CAAA,IACA,EAAA,CAAA,UAAA,KAAA,EAAA,CAAA,KAAA,CAAA,MAAA,GAAA,CADA,IAEA,KAAA,IAAA,EAAA,CAAA,UAAA,GAAA,CAFA,IAGA,KAAA,IAAA,EAAA,CAAA,UAAA,GAAA,CAPA,EAQA;AACA;AACA,eAAA,IAAA;AACA,OAXA,MAWA;AACA,eAAA,KAAA;AACA;AACA;AAtCA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"product-carousel tw-w-full tw-flex tw-flex-col tw-items-center\">\r\n    <div class=\"image-container\">\r\n      <Magnifier\r\n        class=\"tw-w-full tw-h-full tw-object-cover\"\r\n        :src=\"image[indexImage]\"\r\n        :src-large=\"image[indexImage]\"\r\n      >\r\n      </Magnifier>\r\n      <!-- <img class=\"tw-w-full tw-h-full tw-object-cover\" :src=\"image[indexImage]\"/> -->\r\n    </div>\r\n    <div class=\"image-slide tw-h-20 tw-relative tw-flex tw-justify-center mt-4\">\r\n      <div\r\n        class=\"prev tw-h-full tw-flex tw-items-center tw-mr-4 tw-cursor-pointer\"\r\n        @click=\"prevImage\"\r\n      >\r\n        PREV\r\n      </div>\r\n      <div class=\"image tw-flex tw-h-full tw-overflow-hidden\">\r\n        <img\r\n          class=\"tw-h-full tw-w-20 tw-object-cover tw-mr-4\"\r\n          :class=\"{ 'border-focus': index === indexImage }\"\r\n          :src=\"item\"\r\n          v-for=\"(item, index) in image\"\r\n          :key=\"index\"\r\n          @click=\"selectImage(index)\"\r\n          v-show=\"showImage(index)\"\r\n        />\r\n      </div>\r\n\r\n      <div\r\n        class=\"next tw-h-full tw-flex tw-items-center tw-cursor-pointer\"\r\n        @click=\"nextImage\"\r\n      >\r\n        NEXT\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Magnifier from \"@/components/controls/magnifier\";\r\nexport default {\r\n  components: {\r\n    Magnifier,\r\n  },\r\n  props: {\r\n    imageProduct: {\r\n      type: Array,\r\n      default: () => {},\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      image: [\r\n        \"https://didongviet.vn/pub/media/catalog/product//i/p/iphone-13-blue-didongviet.jpg\",\r\n        \"https://didongviet.vn/pub/media/catalog/product//i/p/iphone-13-blue-didongviet.jpg\",\r\n        \"https://didongviet.vn/pub/media/catalog/product//i/p/iphone-13-pink-didongviet.jpg\",\r\n        \"https://didongviet.vn/pub/media/catalog/product//i/p/iphone-13-blue-didongviet.jpg\",\r\n      ],\r\n      indexImage: 0,\r\n    };\r\n  },\r\n  computed: {},\r\n  methods: {\r\n    prevImage() {\r\n      const me = this;\r\n      me.indexImage--;\r\n      if (me.indexImage < 0) {\r\n        me.indexImage = me.image.length - 1;\r\n      }\r\n    },\r\n    nextImage() {\r\n      const me = this;\r\n      me.indexImage++;\r\n      if (me.indexImage > me.image.length - 1) {\r\n        me.indexImage = 0;\r\n      }\r\n    },\r\n    selectImage(index) {\r\n      const me = this;\r\n      me.indexImage = index;\r\n    },\r\n    showImage(index) {\r\n      const me = this;\r\n      console.log(index);\r\n      //nếu chọn ảnh đầu tiên thì hiển thị các ảnh từ vị trí 0-3\r\n      //nếu chọn ảnh cuối thì hiển thị các ảnh từ vị trí cuối đến cuối -3\r\n      if (\r\n        (me.indexImage === 0 && index < 3) ||\r\n        (me.indexImage === me.image.length - 1 &&\r\n          index > me.image.length - 1 - 3) ||\r\n        (me.indexImage !== 0 &&\r\n          me.indexImage !== me.image.length - 1 &&\r\n          index >= me.indexImage - 1 &&\r\n          index <= me.indexImage + 1)\r\n      ) {\r\n        // || (me.indexImage!==0 && me.indexImage!==me.image.length && (index))\r\n        return true;\r\n      } else {\r\n        return false;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.product-carousel {\r\n  height: 600px;\r\n\r\n  .image-container {\r\n    z-index: 2;\r\n    width: 500px;\r\n    height: calc(100% - 81px - 17px);\r\n  }\r\n  .image-slide {\r\n    width: 500px;\r\n  }\r\n}\r\n.border-focus {\r\n  border: 1px solid red;\r\n}\r\n</style>"],"sourceRoot":"src/components/controls/product-carousel"}]}