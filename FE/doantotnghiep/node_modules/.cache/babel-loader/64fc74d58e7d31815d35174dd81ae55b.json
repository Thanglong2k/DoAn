H,yCAqFmCjB,KArFnC,EAqFyC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAdC,gBAAAA,MAAc,SAAdA,MAAc;AAAA;AAAA,uBACnC7B,UAAU,CAAC6C,sBAAX,CAAkCjB,KAAlC,EAAyCG,IAAzC,CAA8C,UAACC,GAAD,EAAO;AAEvDH,kBAAAA,MAAM,CAAC,wBAAD,EAA0BG,GAAG,CAAChB,IAAJ,CAASiB,IAAnC,CAAN;AACH,iBAHK,EAGHC,KAHG,CAGG,UAACC,CAAD,EAAK;AACVC,kBAAAA,OAAO,CAACC,GAAR,CAAYF,CAAZ;AACH,iBALK,CADmC;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAO5C,KA5FG;;AA6FJ;AACR;AACA;AACA;AACA;AACcW,IAAAA,qBAlGF,wCAkGiClB,KAlGjC,EAkGuC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAdC,gBAAAA,MAAc,SAAdA,MAAc;AAAA;AAAA,uBACjC7B,UAAU,CAAC8C,qBAAX,CAAiClB,KAAjC,EAAwCG,IAAxC,CAA6C,UAACC,GAAD,EAAO;AACtDH,kBAAAA,MAAM,CAAC,aAAD,EAAeG,GAAG,CAAChB,IAAJ,CAASiB,IAAT,CAAcA,IAA7B,CAAN;AACAJ,kBAAAA,MAAM,CAAC,iBAAD,EAAmBG,GAAG,CAAChB,IAAJ,CAASiB,IAAT,CAAcQ,WAAjC,CAAN;AACAZ,kBAAAA,MAAM,CAAC,cAAD,EAAgBG,GAAG,CAAChB,IAAJ,CAASiB,IAAT,CAAcS,SAA9B,CAAN;AACH,iBAJK,EAIHR,KAJG,CAIG,UAACC,CAAD,EAAK;AACVC,kBAAAA,OAAO,CAACC,GAAR,CAAYF,CAAZ;AACH,iBANK,CADiC;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQ1C,KA1GG;;AA2GJ;AACR;AACA;AACA;AACA;AACcY,IAAAA,oBAhHF,uCAgHgCnB,KAhHhC,EAgHsC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAdC,gBAAAA,MAAc,SAAdA,MAAc;AAAA;AAAA,uBAChC7B,UAAU,CAAC+C,oBAAX,CAAgCnB,KAAhC,EAAuCG,IAAvC,CAA4C,UAACC,GAAD,EAAO;AACrDH,kBAAAA,MAAM,CAAC,uBAAD,EAAyBG,GAAG,CAAChB,IAAJ,CAASiB,IAAlC,CAAN;AAEH,iBAHK,EAGHC,KAHG,CAGG,UAACC,CAAD,EAAK;AACVC,kBAAAA,OAAO,CAACC,GAAR,CAAYF,CAAZ;AACH,iBALK,CADgC;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAOzC,KAvHG;AAwHJa,IAAAA,aAxHI,gCAwHmBpB,KAxHnB,EAwHyB;AAAA,UAAdC,MAAc,SAAdA,MAAc;AACzB7B,MAAAA,UAAU,CAACiD,aAAX,CAAyBrB,KAAzB,EAAgCG,IAAhC,CAAqC,UAACC,GAAD,EAAO;AACxC,eAAO,IAAP;AACH,OAFD,EAEGE,KAFH,CAES,UAACC,CAAD,EAAK;AACVC,QAAAA,OAAO,CAACC,GAAR,CAAYF,CAAZ;AACH,OAJD;AAKH,KA9HG;AA+HJe,IAAAA,aA/HI,iCA+HmBtB,KA/HnB,EA+HyB;AAAA,UAAdC,MAAc,UAAdA,MAAc;AACzB7B,MAAAA,UAAU,CAACkD,aAAX,CAAyBtB,KAAK,CAACuB,EAA/B,EAAkCvB,KAAK,CAACwB,QAAxC,EAAkDrB,IAAlD,CAAuD,UAACC,GAAD,EAAO;AAC1D,eAAO,IAAP;AACH,OAFD,EAEGE,KAFH,CAES,UAACC,CAAD,EAAK;AACVC,QAAAA,OAAO,CAACC,GAAR,CAAYF,CAAZ;AACH,OAJD;AAKH,KArIG;AAsIJkB,IAAAA,aAtII,iCAsImBF,EAtInB,EAsIsB;AAAA,UAAXtB,MAAW,UAAXA,MAAW;AACtB7B,MAAAA,UAAU,CAACsD,MAAX,CAAkBH,EAAlB,EAAsBpB,IAAtB,CAA2B,UAACC,GAAD,EAAO;AAC9B,eAAO,IAAP;AACH,OAFD,EAEGE,KAFH,CAES,UAACC,CAAD,EAAK;AACVC,QAAAA,OAAO,CAACC,GAAR,CAAYF,CAAZ;AACA,eAAO,KAAP;AACH,OALD;AAMH;AA7IG;AA3DE,CAAd","sourcesContent":["import ProductAPI from '@/apis/components/ProductApi.js'\r\nexport default{\r\n    namespaced:true,\r\n    state:{\r\n        products:[],//Danh sách sản phẩm\r\n        product:null,//Sản phẩm\r\n        newProducts:[],//Danh sách sản phẩm mới\r\n        bestSellingProducts:[],//Danh sách sản phẩm bạn nhất\r\n        productTotal:0,//số lượng sản phẩm lấy ra\r\n        pageTotal:0,//số lượng trang\r\n        relatedProducts:[],//Danh sách sản phẩm liên quan cùng hãng SX\r\n        filter:[],//bộ lọc sản phẩm\r\n        compareProducts:[],//Dánh sách sản phẩm so sánh\r\n        recommoandProducts:[],//Dánh sách sản phẩm gợi ý so sánh\r\n    },\r\n    getters:{},\r\n    mutations:{\r\n        //set giá trị bộ filter\r\n        setFilter(state,data){\r\n            state.filter=data\r\n        },\r\n        //set giá trị danh sách sản phẩm so sánh\r\n        setCompareProducts(state,data){\r\n            \r\n            state.compareProducts=data\r\n            \r\n        },\r\n        //set giá trị cho danh sách sản phẩm\r\n        setProductByID(state,data){\r\n            state.product=data\r\n        },\r\n        //set giá trị cho danh sách sản phẩm\r\n        setProducts(state,data){\r\n            state.products=data\r\n        },\r\n        //set giá trị cho danh sách sản phẩm mới\r\n        setNewProducts(state,data){\r\n            state.newProducts=data\r\n        },\r\n        //set giá trị cho danh sách sản phẩm liên quan cùng hãng SX\r\n        setRelatedProducts(state,data){\r\n            state.relatedProducts=data\r\n        },\r\n        //set giá trị cho danh sách sản phẩm gợi ý so sánh\r\n        setRecommoandProducts(state,data){\r\n            state.recommoandProducts=data\r\n        },\r\n        //set giá trị cho danh sách sản phẩm bán chạy\r\n        setBestSellingProducts(state,data){\r\n            state.bestSellingProducts=data\r\n        },\r\n        //set giá trị cho tổng sản phẩm lấy ra\r\n        setProductTotal(state,data){\r\n            state.productTotal=data\r\n        },\r\n        //set giá trị cho tổng trang\r\n        setPageTotal(state,data){\r\n            state.pageTotal=data\r\n        }\r\n    },\r\n    actions:{\r\n        /**\r\n         * Lấy danh sách sản phẩm\r\n         * @param {*} param\r\n         * @param {*} param\r\n         * @author TTLONG\r\n         * CreatedDate 23/02/2022  \r\n         */\r\n        async getProductByID({commit},param){\r\n            await ProductAPI.getById(param).then((res)=>{\r\n                \r\n                commit('setProductByID',res.data.Data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        /**\r\n         * Lấy danh sách sản phẩm\r\n         * @param {*} param\r\n         * @param {*} param\r\n         * @author TTLONG\r\n         * CreatedDate 23/02/2022  \r\n         */\r\n        async getProducts({commit},param){\r\n            await ProductAPI.getAll().then((res)=>{\r\n                \r\n                commit('setProducts',res.data.Data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        /**\r\n         * Lấy danh sách sản phẩm phân trang\r\n         * @param {*} param\r\n         * @param {*} param\r\n         * @author TTLONG\r\n         * CreatedDate 23/02/2022  \r\n         */\r\n        async getProductsPaging({commit},param){\r\n            await ProductAPI.getProductsPaging(param).then((res)=>{\r\n                const data=res?.data?.Data\r\n                commit('setProducts',data.Data)\r\n                commit('setProductTotal',data.TotalRecord)\r\n                commit('setPageTotal',data.TotalPage)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        /**\r\n         * Lấy danh sách sản phẩm mới nhất\r\n         * @param {*} param\r\n         * @param {*} param\r\n         * @author TTLONG\r\n         * CreatedDate 23/02/2022  \r\n         */\r\n         async getNewProducts({commit},param){\r\n            await ProductAPI.getNewProducts(param).then((res)=>{\r\n                \r\n                commit('setNewProducts',res.data.Data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        /**\r\n         * Lấy danh sách sản phẩm liên quan cùng hãng sản xuất\r\n         * @param {*} param\r\n         * @param {*} param\r\n         * @author TTLONG\r\n         * CreatedDate 04/03/2022  \r\n         */\r\n         async getRelatedProducts({commit},param){\r\n            await ProductAPI.getRelatedProducts(param).then((res)=>{\r\n                \r\n                commit('setRelatedProducts',res.data.Data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        /**\r\n         * Lấy danh sách sản phẩm bán chạy nhất\r\n         * @param {*} param\r\n         * @param {*} param\r\n         * @author TTLONG\r\n         * CreatedDate 23/02/2022  \r\n         */\r\n         async getBestSellingProducts({commit},param){\r\n            await ProductAPI.getBestSellingProducts(param).then((res)=>{\r\n                \r\n                commit('setBestSellingProducts',res.data.Data)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        /**\r\n         * Lấy danh sách sản phẩm theo danh mục\r\n         * @author TTLONG\r\n         * CreatedDate 26/02/2022\r\n         */\r\n        async getProductsByCategory({commit},param){\r\n            await ProductAPI.getProductsByCategory(param).then((res)=>{\r\n                commit('setProducts',res.data.Data.Data)\r\n                commit('setProductTotal',res.data.Data.TotalRecord)\r\n                commit('setPageTotal',res.data.Data.TotalPage)\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        /**\r\n         * Lấy danh sách sản phẩm gợi ý so sánh\r\n         * @author TTLONG\r\n         * CreatedDate 26/02/2022\r\n         */\r\n        async getRecommendProducts({commit},param){\r\n            await ProductAPI.getRecommendProducts(param).then((res)=>{\r\n                commit('setRecommoandProducts',res.data.Data)\r\n                \r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        insertProduct({commit},param){\r\n            ProductAPI.createProduct(param).then((res)=>{\r\n                return true\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        updateProduct({commit},param){\r\n            ProductAPI.updateProduct(param.id,param.formData).then((res)=>{\r\n                return true\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n            })\r\n        },\r\n        deleteProduct({commit},id){\r\n            ProductAPI.delete(id).then((res)=>{\r\n                return true\r\n            }).catch((e)=>{\r\n                console.log(e)\r\n                return false\r\n            })\r\n        }\r\n    }\r\n}"]}]}