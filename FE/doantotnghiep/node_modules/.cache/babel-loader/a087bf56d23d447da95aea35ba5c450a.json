t-area tw-mt-4\" v-else>\r\n      <div class=\"recommand-text tw-text-xs tw-text-gray-400\">\r\n        Gợi ý sản phẩm so sánh\r\n      </div>\r\n      <div\r\n        class=\"\r\n          recommand-product\r\n          tw-h-80 tw-grid tw-grid-cols-2 tw-grid-rows-2 tw-gap-2 tw-mt-2\r\n        \"\r\n      >\r\n        <div\r\n          class=\"\r\n            product-item\r\n            tw-w-full tw-h-full tw-p-4 tw-cursor-pointer\r\n            hover:tw-bg-red-50\r\n          \"\r\n          v-for=\"item in recommoandProducts\"\r\n          :key=\"item.ProductID\"\r\n          @click=\"chooseCompareProduct(item)\"\r\n        >\r\n          <div class=\"product-item-image\">\r\n            <img\r\n              class=\"tw-h-full tw-w-full tw-object-contain\"\r\n              :src=\"item.Image\"\r\n            />\r\n          </div>\r\n          <div class=\"product-item-name tw-pt-2 tw-h-5 tw-text-center\">\r\n            {{ item.ProductName }}\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapMutations } from \"vuex\";\r\nexport default {\r\n  props: {\r\n    product: {\r\n      type: Object,\r\n      default: () => {},\r\n    },\r\n    \r\n  },\r\n  data() {\r\n    return {\r\n      keywork: \"\",\r\n      productData:{},\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState(\"product\", [\"recommoandProducts\",\"compareProducts\"]),\r\n  },\r\n  created() {\r\n    const me = this;\r\n    me.productData=JSON.parse(JSON.stringify(this.product))\r\n    if (Object.keys(me.productData).length > 0) {\r\n      me.keywork = me.productData.ProductName + \" \" + me.productData.ProductAttributes[0].Memory;\r\n    }\r\n  },\r\n  mounted() {\r\n    const me = this;\r\n    \r\n  },\r\n  methods: {\r\n    ...mapMutations(\"product\",[\"setCompareProducts\"]),\r\n    /**\r\n     * Xem chi tiết sản phẩm\r\n     */\r\n    viewProductDetail() {\r\n      const me = this;\r\n      localStorage.setItem(\"Id_Product\", me.productData.ProductID);\r\n      me.$router.push({\r\n        name: \"ProductDetail\",\r\n        params: { productMeta: me.productData.Meta },\r\n      });\r\n    },\r\n    /**\r\n     * Chọn sản phẩm so sánh\r\n     */\r\n    chooseCompareProduct(product){\r\n      const me=this\r\n      const compareProduct=JSON.parse(sessionStorage.getItem('CompareProduct'))\r\n      compareProduct.push(product)\r\n      me.setCompareProducts(compareProduct)\r\n      me.keywork=product.ProductName\r\n      me.productData=product\r\n      sessionStorage.setItem('CompareProduct',JSON.stringify(compareProduct))\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import '@/styles/mixins/mixins.scss';\r\n\r\n.compare-product-contain {\r\n  width: 45%;\r\n  .product-info {\r\n    .product-image {\r\n      img {\r\n      }\r\n    }\r\n  }\r\n  .recommand-product-area {\r\n    .recommand-product {\r\n      .product-item {\r\n        @include border();\r\n        .product-item-image {\r\n          height: calc(100% - 21px);\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":"src/components/client/views/compare-product"}]}