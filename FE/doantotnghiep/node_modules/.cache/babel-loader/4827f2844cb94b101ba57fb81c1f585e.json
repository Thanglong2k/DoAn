olumnNumber:{\r\n      type:Number,\r\n      default:4\r\n    },\r\n    type:{\r\n      type:Number,\r\n      default:0\r\n    }\r\n  },\r\n  computed:{\r\n    gridColumn(){\r\n      console.log(this.columnNumber)\r\n      return 'tw-grid-cols-'+this.columnNumber\r\n    }\r\n  },\r\n  methods:{\r\n...mapActions(\"product\", [\"getProductsByCategory\"]),\r\n      /**\r\n     * Tìm kiếm sản phẩm\r\n     */\r\n    async reviewAll(){\r\n      const me = this\r\n      if(me.type===me.$enumeration.AreaCardType.Product){\r\n\r\n        const params = {\r\n          categoryID: '',\r\n          queryText:me.querySearch,\r\n          filter:[],\r\n          pageSize: 10,\r\n          pageIndex: 1,\r\n          sortBy: 0,\r\n        };\r\n        await me.getProductsByCategory(params);\r\n        if(me.$route.fullPath!=='/search'){\r\n          me.$router.push('/search')\r\n        }\r\n      }else{    \r\n        me.$emit(\"viewAllPost\")\r\n      }\r\n      \r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.area-card {\r\n  border-radius: 8px;\r\n  .header {\r\n    border: none;\r\n    .v-card__title {\r\n      padding: 0 !important;\r\n    }\r\n  }\r\n}\r\n</style>\r\n<style >\r\n.header .v-card__title {\r\n  padding: 0 !important;\r\n  font-size:22px !important;\r\n  @apply tw-font-bold !important;\r\n}\r\n</style>"],"sourceRoot":"src/components/controls/area-card"}]}