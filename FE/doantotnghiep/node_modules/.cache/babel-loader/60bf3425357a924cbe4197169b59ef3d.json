r\n    },\r\n    actions: {\r\n        \r\n        getCustomersPaging({commit},params){\r\n            CustomerAPI.getPaging(params).then((res)=>{\r\n                const data=res?.data?.Data\r\n                commit('setCustomers',data.Data)\r\n                commit('setCustomerTotal',data.TotalRecord)\r\n                commit('setPageTotal',data.TotalPage)\r\n            })\r\n        },\r\n        /**\r\n         * Thêm\r\n         * @param {*} param0 \r\n         * @param {*} params \r\n         */\r\n        async  insertCustomer({commit},params){\r\n            return await new Promise((resolve,reject)=>{\r\n            CustomerAPI.insert(params).then((res)=>{\r\n                resolve(true)\r\n            }).catch((e)=>{\r\n                reject(false)\r\n            })\r\n        })\r\n        },\r\n        /**\r\n         * Sửa\r\n         * @param {*} param0 \r\n         * @param {*} id \r\n         */\r\n         updateCustomer({commit},params){\r\n            return new Promise((resolve,reject)=>{\r\n                CustomerAPI.update(params.id,params.params).then((res)=>{\r\n                    resolve(true)\r\n                }).catch((e)=>{\r\n                    reject(false)\r\n                })\r\n\r\n            })\r\n        },\r\n        deleteCustomer({commit},id){\r\n            return new Promise((resolve,reject)=>{\r\n                CustomerAPI.delete(id).then((res)=>{\r\n                    resolve(true)\r\n                }).catch((e)=>{\r\n                    reject(false)\r\n                })\r\n\r\n            })\r\n        },\r\n        // async getCustomerID({commit},param){\r\n        //     await CustomerAPI.getCustomerID(param).then((res)=>{\r\n        //         commit(\"setCustomerID\",res.data.Data)\r\n        //     }).catch((e)=>{\r\n        //         console.log(e);\r\n        //     })\r\n        // },\r\n        // getCustomerByUserName({commit},param){\r\n        //     CustomerAPI.getCustomerByUserName(param).then((res)=>{\r\n        //         commit(\"setCustomer\",res.data.Data)\r\n        //     }).catch((e)=>{\r\n        //         console.log(e);\r\n        //     })\r\n        // }\r\n    }\r\n}"]}]}