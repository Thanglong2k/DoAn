t":"src/components/admin/layouts","sourcesContent":["<template>\r\n  <div class=\"navbar\" :class=\"{ 'not-expend': isToggleNavbar }\">\r\n    <div class=\"navbar-header\">\r\n      <div class=\"navbar-logo\" :class=\"{ 'display': isToggleNavbar }\">\r\n        <div class=\"logo\"></div>\r\n        <span class=\"text\">TTL Admin</span>\r\n      </div>\r\n      <div class=\"navbar-toggle icon-svg\" @click=\"toggleNavbar\"></div>\r\n    </div>\r\n    <div class=\"navbar-menu\">\r\n      <router-link\r\n        class=\"menu-item\"\r\n        v-for=\"(item, index) in navItems\"\r\n        :key=\"index\"\r\n        :to=\"item.link\"\r\n        :title=\"item.text\"\r\n        @click.native=\"setHeader(item)\"\r\n      >\r\n        <div class=\"menu-item-icon icon\" :class=\"item.icon\"></div>\r\n        <div class=\"menu-item-text\" :class=\"{ 'display': isToggleNavbar }\">\r\n          {{ item.text }}\r\n        </div>\r\n      </router-link>\r\n      <!-- <router-link \r\n            class=\"menu-item\" \r\n            to=\"/empmloyee\"\r\n            >\r\n              <div class=\"menu-item-icon\"></div>\r\n              <div class=\"menu-item-text\">long ca</div>\r\n          </router-link>\r\n           <router-link \r\n            class=\"menu-item\" \r\n            to=\"/empmloyee\"\r\n            >\r\n              <div class=\"menu-item-icon\"></div>\r\n              <div class=\"menu-item-text\">long ca</div>\r\n          </router-link>\r\n          <router-link \r\n            class=\"menu-item\" \r\n            to=\"/empmloyee\"\r\n            >\r\n              <div class=\"menu-item-icon\"></div>\r\n              <div class=\"menu-item-text\">long ca</div>\r\n          </router-link> -->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapMutations } from \"vuex\";\r\n\r\n\r\nexport default {\r\n  name: \"Navbar\",\r\n  props: {\r\n    navItems: {\r\n      type: Array,\r\n      default: () => [],\r\n    },\r\n  },\r\n\r\n  data() {\r\n    return {};\r\n  },\r\n  computed: {\r\n    ...mapState('admin',{\r\n      isToggleNavbar: 'toggleNavbar',\r\n    }),\r\n  },\r\n  methods: {\r\n    ...mapMutations('admin',['setToggleNavbar','setHeaderTitle']),\r\n    /**\r\n     * thu/má»Ÿ navbar\r\n     * CreatedBy: TTLONG (8/9/2021)\r\n     */\r\n    toggleNavbar() {\r\n      this.setToggleNavbar();\r\n    },\r\n    setHeader(item){\r\n      const me=this\r\n      me.setHeaderTitle(item.text)\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import url(\"../../../assets/css/navbar.css\");\r\n.navbar {\r\n  background-color: #24344b;\r\n  width: 200px;\r\n  height: 100vh;\r\n  box-sizing: border-box;\r\n  transition: width 0.5s;\r\n  .navbar-header {\r\n    width: 100%;\r\n    height: 53px;\r\n    box-sizing: border-box;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    cursor: pointer;\r\n    .navbar-logo {\r\n      min-width: 150px;\r\n      height: 50px;\r\n      display: flex;\r\n      justify-content: flex-start;\r\n      align-items: center;\r\n      .logo {\r\n        background-size: 35px;\r\n        background-image: url(\"../../../assets/icons/ic_thietbi_56.svg\");\r\n        background-repeat: no-repeat;\r\n        background-position: center;\r\n        height: 100%;\r\n        width: 35px;\r\n        margin-left: 12px;\r\n      }\r\n      .text {\r\n        height: 100%;\r\n        padding-left: 8px;\r\n        display: flex;\r\n        align-items: center;\r\n        font-weight: 600;\r\n        font-size: 16px;\r\n        color: #f3f4f6;\r\n      }\r\n    }\r\n    .navbar-toggle {\r\n      width: 50px;\r\n      height: 100%;\r\n      background-position: 0px 0px;\r\n    }\r\n  }\r\n  .navbar-menu {\r\n    width: 100%;\r\n    height: calc(100% - 54px);\r\n    box-sizing: border-box;\r\n    padding-top: 12px;\r\n    .menu-item {\r\n      width: inherit;\r\n      height: 40px;\r\n      margin-bottom: 6px;\r\n      border-left: 4px solid #24344b;\r\n      display: flex;\r\n      box-sizing: border-box;\r\n      cursor: pointer;\r\n      text-decoration: none;\r\n      &:hover {\r\n        background-color: #404f64;\r\n        border-color: #404f64;\r\n      }\r\n      .menu-item-icon {\r\n        width: 30px;\r\n        height: 40px;\r\n        background-position: center;\r\n        margin: 0 8px;\r\n      }\r\n      .menu-item-text {\r\n        width: calc(100% - 47px);\r\n        height: 40px;\r\n        display: flex;\r\n        align-items: center;\r\n        color: #c8c8c8;\r\n        animation: slide-down 1.75s ease;\r\n      }\r\n    }\r\n  }\r\n}\r\n.router-link-exact-active {\r\n  border-left-color: #0dd469 !important;\r\n  background-color: #404f64;\r\n}\r\n.router-link-exact-active .menu-item-text {\r\n  color: #15c068;\r\n  font-family: OpenSansBold;\r\n}\r\n.router-link-active {\r\n  border-left-color: #0dd469 !important;\r\n  background-color: #404f64;\r\n}\r\n.router-link-active .menu-item-text {\r\n  color: #15c068;\r\n  font-family: OpenSansBold;\r\n}\r\n\r\n\r\n.not-expend {\r\n  width: 50px;\r\n}\r\n.display{\r\n  display: none !important;\r\n  \r\n}\r\n@keyframes slide-down {\r\n            0% {\r\n                opacity: 0;\r\n            }\r\n  \r\n            100% {\r\n                opacity: 1;\r\n            }\r\n        }\r\n/* :style=\"[isToggleNavbar?{'display':'none'}:{'display':'block'}]\" */\r\n</style>"]}]}